    "jest:update": "./node_modules/.bin/jest -c=./jest.config.js -u",
    "jest:watch": "./node_modules/jest/bin/jest.js -c=./jest.config.js --watch",
    "lint-fix": "npm run lint:es -- --fix",
    "lint:cache": "eslint --ext .js,.jsx . --cache && npm run lint:sass",
    "lint:es": "eslint --ext .js,.jsx .",
    "lint:sass": "if [ -z \"$JENKINS_URL\" ]; then sass-lint -v -q; else sass-lint -v -q --format checkstyle --output reports/sassLint.xml; fi",
    "lint": "npm run lint:es && npm run lint:sass",
    "localApi": "npm install && npm start -- --proxy=localApi",
    "mocha:debug": "NODE_PATH=. mocha --inspect-brk --require test/unit/nextGenSetup.js --watch test/unit/setup.js",
    "mocha:update": "NODE_PATH=. mocha --update --require test/unit/nextGenSetup.js --watch test/unit/setup.js",
    "mocha": "NODE_PATH=. mocha --timeout 5000 --require test/unit/nextGenSetup.js --watch test/unit/setup.js",
    "mock-data-docs": "jsdoc mocks/services -r -d mock-data-docs",
    "mock-server": "NODE_PATH=. gulp e2e --mock_services_only",
    "mock": "npm install && npm start",
    "postinstall": "cp bin/pre-commit .git/hooks/pre-commit",
    "pr": "node ./bin/create-pr/index.js",
    "prebuild": "npm run check-engines-version",
    "prestart": "npm run check-engines-version",
    "pretest": "npm install --no-optional && npm run lint && npm run flow check",
    "prettier": "prettier --write",
    "ptest1": "npm start -- --proxy=ptest1.qa",
    "ptest2": "npm start -- --proxy=ptest2.qa",
    "qa1": "npm start -- --proxy=qa1",
    "qa3": "npm start -- --proxy=qa3",
    "qa4": "npm start -- --proxy=qa4",
    "qa5": "npm start -- --proxy=qa5",
    "qa6": "npm start -- --proxy=qa6",
    "retire": "retire -cp || exit 0;",
    "sonarqube": "swa-build-scripts sonarqube",
    "start-jenkins": "ssh developer@172.18.90.164 'bash -s' < ./bin/start-mWeb-jenkins.sh",
    "start": "NODE_PATH=. gulp dev",
    "storybook:eval": "STORYBOOK_DEVTOOL=eval STORYBOOK_SOURCEMAPS=true npm run storybook",
    "storybook": "./g iconfont && NODE_PATH=. start-storybook -p ${STORYBOOK_PORT:=9200} ${STORYBOOK_SOURCEMAPS:=false} ${STORYBOOK_DEVTOOL:=inline-source-map} -c storybook -s ./src,./mocks/wcm/wcm",
    "test:coverage": "npm run test:jest && npm run test:mocha:coverage",
    "test:diff": "docker run --user 1000 --shm-size 8g --net=host --rm -v $(pwd):/src -t backstopjs/backstopjs:6.3.3 --config=test/ui/backstop.config.js test --port=9200 --filter",
    "test:functional": "npm run test:functionals-update && ./g e2e --test",
    "test:functionals-group": "npm run test:functionals-update && ./g e2e --group",
    "test:functionals-update": "node_modules/.bin/webdriver-manager update --versions.standalone 3.141.59 --ignore_ssl",
    "test:functionals": "npm run test:functionals-update && NODE_PATH=. NODE_TLS_REJECT_UNAUTHORIZED=0 gulp e2e",
    "test:jest": "./node_modules/.bin/jest -c=./jest.config.js",
    "test:live": "node_modules/.bin/webdriver-manager update --ignore_ssl && NODE_PATH=. NODE_TLS_REJECT_UNAUTHORIZED=0 gulp e2eLive",
    "test:mocha:coverage": "./bin/unit_test.js --coverage",
    "test:mocha:update": "./bin/unit_test.js --update",
    "test:mocha": "./bin/unit_test.js",
    "test:ui": "./bin/ui_test.js --port=9200",
    "test:ui-dev": "./bin/ui_test.js --port=9200 --threads=10",
    "test:unit": "npm run test:jest && npm run test:mocha",
    "test:update": "npm run test:mocha:update && npm run jest:update",
    "test": "npm run test:unit && npm run build && npm run test:functionals",
    "which-api-version": "cd bin/which-api-version && npm start -s"
  },
