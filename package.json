  "scripts": {
    "api-contract-test": "NODE_PATH=. BABEL_DISABLE_CACHE=1 mocha test/apiContract/setup.js \"test/apiContract/*Specs.js\" --require @babel/register -t 10000 -R mocha-multi --reporter-options spec=-,xunit=reports/contract-tests.xml",
